{
  "name": "webtoolsnz/yii2-admin-lte",
  "description": "Yii2 Integration kit for Admin LTE theme",
  "keywords": [
    "yii2",
    "theme",
    "admin-lte"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "homepage": "https://github.com/webtoolsnz/yii2-admin-lte",
  "authors": [
    {
      "name": "Byron Adams",
      "email": "byron@webtools.co.nz",
      "role": "Developer"
    },
    {
      "name": "Webtools Ltd",
      "email": "support@webtools.co.nz",
      "homepage": "http://www.webtools.co.nz",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.7",
    "yiisoft/yii2-bootstrap": "*",
    "almasaeed2010/adminlte": "^2.3",
    "fortawesome/font-awesome": "^4.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "fabpot/php-cs-fixer": "^1.11",
    "symfony/dom-crawler": "^2.8",
    "symfony/css-selector": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "webtoolsnz\\AdminLte\\": "src",
      "webtoolsnz\\AdminLte\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --coverage-text",
    "php-cs-fixer" : "./vendor/bin/php-cs-fixer fix . || true"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
